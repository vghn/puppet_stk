#!/usr/bin/env bash
# Clean-up tasks
#
# USAGE: ${BASH_SOURCE[0]} [all | ami]

# Load environment
# shellcheck disable=1090
. "$(cd "$(dirname "${BASH_SOURCE[0]}")/../" && pwd -P)/envrc"

# Process arguments
case "${1:-}" in
  all)
    clean_all
    ;;
  ami)
    ami_clean
    ;;
  *)
    clean_all
    ;;
esac
