#!/usr/bin/env bash
# Clean-up tasks

# Load environment
# shellcheck disable=1090
. "$(cd "$(dirname "${BASH_SOURCE[0]}")/../" && pwd -P)/envrc"

# Process arguments
case "${1:-}" in
  all)
    ami_clean
    ;;
  ami)
    ami_clean
    ;;
  *)
    e_abort "USAGE: ${BASH_SOURCE[0]} [all | ami]"
    ;;
esac
